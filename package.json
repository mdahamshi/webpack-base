{
  "name": "webpack-base",
  "version": "1.0.0",
  "description": "A webpack base repo, includes basic plugins and prettier, ESlint.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdahamshi/webpack-base.git"
  },
  "scripts": {
    "format": "npx prettier --write .",
    "test": "jest",
    "build": "npm run format && webpack --config webpack.prod.js",
    "dev": "webpack serve --open --config webpack.dev.js",
    "deploy": "git subtree push --prefix dist origin gh-pages"
  },
  "keywords": [],
  "author": "SaraWebs",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@eslint/css": "^0.9.0",
    "@eslint/js": "^9.29.0",
    "babel-jest": "^30.0.4",
    "css-loader": "^7.1.2",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^30.0.4",
    "prettier": "3.6.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@sarawebs/sb-utils": "^1.0.1"
